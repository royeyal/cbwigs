{
  "name": "cbwigs-gsap-animations",
  "version": "1.0.0",
  "description": "GSAP animations for Webflow project",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.ts --fix",
    "lint:css": "stylelint 'src/**/*.css' --fix",
    "format": "prettier --write 'src/**/*.{js,ts,css,scss}'",
    "prepare": "husky install",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.19",
    "cssnano": "^6.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "stylelint": "^16.3.1",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "dependencies": {
    "gsap": "^3.12.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "gsap",
    "animations",
    "webflow",
    "javascript",
    "css"
  ],
  "author": "",
  "license": "MIT"
}